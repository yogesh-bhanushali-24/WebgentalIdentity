@model WebgentalIdentity.Models.Category

@{
    ViewData["Title"] = "CreateCategory";
    Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}

<h1>CreateCategory</h1>

<h4>Category</h4>

<div class="card col d-flex justify-content-center">
    <div class="card-body text-lg-center w-50">
        <form asp-action="CreateCategory">
            <h5 class="card-title">Category Name</h5>
            <p class="card-text"><input asp-for="Cname" class="form-control" /></p>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>





